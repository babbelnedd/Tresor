<UserControl x:Class="Tresor.View.PanelView" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:tresor="clr-namespace:Tresor" xmlns:userControls="clr-namespace:Tresor.View.UserControls" Focusable="False" d:DesignHeight="300" d:DesignWidth="300" mc:Ignorable="d">
    <Grid>
        <ItemsControl ItemsSource="{Binding Passwords}">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <userControls:PasswordTemplate />
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </Grid>
</UserControl>